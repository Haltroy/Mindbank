<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Mindbank.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Mindbank.Views.MainWindow"
        Icon="/Assets/logo.ico"
        TransparencyLevelHint="AcrylicBlur,Blur,Transparent,None"
        Closing="Window_OnClosing"
        Title="Mindbank">
    <views:MainView IsOnDesktop="True" DesktopContainer="{CompiledBinding  $parent[views:MainWindow]}" />
</Window>